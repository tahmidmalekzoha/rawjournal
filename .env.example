# === RawJournal MT5 Workers — Environment Variables ===

# Supabase (self-hosted)
SUPABASE_URL=https://api.rawjournal.pro
SUPABASE_SERVICE_KEY=your-service-role-key-here

# Redis (internal Docker network — do not change unless running Redis externally)
REDIS_URL=redis://redis:6379

# Encryption (generate: python3 -c "import os; print(os.urandom(32).hex())")
ENCRYPTION_KEY=your-32-byte-hex-key-here

# Demo account for market data worker (#4)
# Leave empty to disable market data worker
DEMO_MT5_LOGIN=
DEMO_MT5_PASSWORD=
DEMO_MT5_SERVER=

# Sync intervals (seconds) — defaults are fine for production
SYNC_INTERVAL=15
FULL_CHECK_INTERVAL=60
CANDLE_UPDATE_INTERVAL=30
GRACE_PERIOD=300
TERMINAL_RESTART_HOURS=5
